    "start": "HTTPS=true node scripts/start.js",
    "build-client": "node scripts/build.js",
    "build-server": "node scripts/build.js --server",
    "run-build-client": "npm run build-client",
    "run-build-server": "npm run build-server",
    "run-build": "node build/server/index.js",
    "run-stage-build": "node build/server/index.js --without-csp",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --colors --coverage",
    "build-css": "node-sass --include-path scss src/client/scss/_style.scss src/common/public/static/css/custom/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "storybook": "start-storybook -p 6006 -c config/storybook",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.tsx",
      "!src/client/index.tsx",
      "!src/registerServiceWorker.ts",
      "!src/client/components/App.tsx",
      "!src/client/components/index.ts",
      "!src/client/store/actions.ts",
      "!src/client/store/configure.ts",
      "!src/client/store/middlewares.ts",
      "!src/client/store/abilities.ts",
      "!src/client/store/localization.ts",
      "!src/client/store/reducer.ts",
      "!src/client/store/sagas.ts",
      "!src/client/store/selectors.ts",
      "!src/client/store/states.ts",
      "!src/client/translations/**/*.ts",
      "!src/client/containers/common/Theme/themes.ts",
      "!src/client/services/validation/validators/user/*.ts",
      "!src/client/services/validation/validators/loader.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^(components|containers)$": "<rootDir>/config/jest/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/config/jest/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/config/jest/actionsMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "watchPlugins": [
      "/Volumes/SP Bolt B75/Work/reactjs/test/node_modules/jest-watch-typeahead/filename.js",
      "/Volumes/SP Bolt B75/Work/reactjs/test/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
